[
  {
    "description":" 返回带cookie信息的get请求",
    "request":{
      "uri":"/get/response/cookies",
      "method":"get"
    },
    "response":{
      "cookies":{
        "login":"true",
        "time":"123456"
      },
      "text":"login sucessful"
    }
  },
  {
    "description":"带cookies信息的get请求",
    "request":{
      "uri":"/get/cookies",
      "method":"get",
      "cookies":{
        "name":"wangwu",
        "pwd":"123"
      }
    },
    "response":{
      "text":"带cookies的get请求"
    }
  },
  {
    "description":" 带cookies信息的post请求",
    "request":{
      "uri":"/post/cookies",
      "method":"post",
      "cookies":{
        "name":"zhaoliu",
        "pwd":"456"
      },
      "json":{
        "name":"fengqi",
        "age":"111"
      }
    },
    "response":{
      "status":"200",
      "json":{
        "zhaoliu":"sucessful",
        "status":"1"
      }
    }
  },
  {
    "description":"带请求头的post 方法",
    "request":{
      "uri":"/post/headers",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      }
    },
    "response":{
      "json":{
        "fengqi":"sucessful",
        "status":"2000"
      }
    }
  },
  {
    "description":"重定向到百度",
    "request":{
      "uri":"/redirect"
    },
    "redirectTo":"http://www.baidu.com"
  },
  {
    "description":"这是被重定向的请求",
    "request":{
      "uri":"/redirect/new"
    },
    "response":{
      "text":"重定向成功啦。。"
    }
  }
]